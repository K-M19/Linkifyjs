{
    "extends": [
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "env": {
        "browser": true,
        "node": true,
        "jquery": true,
        "es6": true,
        "amd": true
    },
    "rules": {
        "curly": 2,
        "eqeqeq": 2,
        "no-eq-null": 2,
        "quotes": [2, "single", "avoid-escape"],
        "semi": 2
    },
    "globals": {
        "__base": false,
        "__TLDS__": false,
        "after": false,
        "afterEach": false,
        "before": false,
        "beforeEach": false,
        "expect": false,
        "describe": false,
        "it": false
    }
}
